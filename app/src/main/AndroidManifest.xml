<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aris.kuryerol">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MessageActivity"></activity>
        <activity android:name=".activities.MaketDialog" />
        <activity android:name=".activities.Marshrut" />
        <activity android:name=".activities.DeliveredOrder" />
        <activity android:name=".activities.OrderAccept"
        android:parentActivityName=".activities.Order"/>
        <activity android:name=".activities.Order"
        android:parentActivityName=".activities.drawermenu.StartWork"/>
        <activity android:name=".activities.drawermenu.FasileActivity"
            android:parentActivityName=".activities.MainActivity"></activity>
        <activity android:name=".activities.drawermenu.SifarishlerimActivitiy"
            android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".activities.drawermenu.StartWork"
        android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".activities.drawermenu.CallOfficceActivity"
        android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".activities.drawermenu.OnlineChatActivity"
        android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".activities.drawermenu.ArchiveActivity"
        android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".activities.Authentication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>